{{>_header}}
<div class="top-section">
  <img href="{{avatar}}">
  <form class="searchMessages" action="#" method="POST">
    <input type="text" placeholder="Search your notes!">
    <button type="submit" name="button">Search</button>
  </form>
  <a href="/logout"><button>Logout</button></a>
</div>
<div class="bottom-section">
  <div class="left">
    <a href="/contacts"><button>Contacts</button></a>
    <br>
    <a href="/compose"><button>New</button></a>

  </div>
  <div class="right">
    {{^messages}}
    <h4>No new notes at this time! Hit New to start a new one.</h4>
    {{/messages}}
    {{#messages}}
      {{>_message}}
    {{/messages}}
    <p>Copyright SEC Services 2017</p>
  </div>


</div>
{{>_footer}}
