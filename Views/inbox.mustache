{{>_header}}
    <div class = "topRow">
        {{avatar}}
        <form class = "searchMessages" action = "/login" method = "POST">
        <input type = "text" placeholder= "Search your notes!">
        <input type = "submit" value = "Search!">
        </form>
        <a href = "http://localhost:3000/logout"><button>Logout</button></a>
    </div>
    <div class = "bottomRow">
        <a href = "http://localhost:3000/contacts"><button>View all Contacts!</button></a>
        <br>
        <a href = "http://localhost:3000/compose"><button>Compose a new Message!</button></a>   
        <div class = "messages">
            <h2>This is where the messages should go!</h2>
            {{incoming}} 
            <p>Copyright SEC Services 2017</p>
    </div>
{{>_footer}}